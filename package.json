{
    "publisher": "bsiever",
    "name": "edacation-dlacd",
    "displayName": "EDAcation",
    "description": "Learning environment for digital hardware design with Yosys and nextpnr.",
    "repository": "github:digital-logic-and-computer-design/vscode-edacation",
    "license": "MIT",
    "version": "0.5.8",
    "contributors": [
        "Danielle Huisman <danielle@huisman.me> (https://github.com/DanielleHuisman)",
        "Daan Middelkoop <daantje676@gmail.com>",
        "Bram Wesselink <wesselinkbram@gmail.com>",
        "Mike Almeloo <git@mikealmel.ooo>",
        "Hendrik Folmer <h.h.folmer@utwente.nl>", 
        "Bill Siever <bsiever@gmail.com>; Hacked for just stats view"
    ],
    "engines": {
        "vscode": "^1.73.0"
    },
    "icon": "resources/icons/edacation.png",
    "categories": [
        "Education",
        "Other",
        "Visualization"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./dist/extension/extension.js",
    "browser": "./dist/extension/extension.js",
    "contributes": {
        "customEditors": [
            {
                "viewType": "edacation.yosys",
                "displayName": "Yosys",
                "selector": [
                    {
                        "filenamePattern": "*.stats.json"
                    }
                ],
                "priority": "default"
            }
        ]
    },
    "scripts": {
        "postinstall": "sed -i '' \"s/require('topsort');/require('topsort').default;/\" node_modules/yosys2digitaljs/dist/index.js",
        "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. .",
        "watch-web": "webpack --watch --mode development --devtool eval-source-map",
        "compile-web": "tsc && webpack --mode development --devtool eval-source-map",
        "package-web": "webpack --mode production --devtool source-map",
        "vscode:prepublish": "npm run package-web",
        "lint": "eslint ./src --ext ts",
        "test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/extension/test/suite/index.js",
        "pretest": "npm run compile-web",
        "prepackage": "webpack --mode production",
        "package": "vsce package"
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/mocha": "^10.0.6",
        "@types/path-browserify": "^1.0.2",
        "@types/tar-fs": "^2.0.4",
        "@types/vscode": "1.73.0",
        "@types/vscode-webview": "^1.57.4",
        "@types/webpack-env": "^1.18.4",
        "@typescript-eslint/eslint-plugin": "^7.13.0",
        "@typescript-eslint/parser": "^7.13.0",
        "@vscode/test-web": "^0.0.54",
        "@vue/compiler-sfc": "^3.4.13",
        "@vue/tsconfig": "^0.5.1",
        "assert": "^2.1.0",
        "css-loader": "^7.1.2",
        "eslint": "^8.57.0",
        "eslint-plugin-vue": "^9.22.0",
        "mini-css-extract-plugin": "^2.7.6",
        "mocha": "^10.2.0",
        "prettier": "^3.3.2",
        "process": "^0.11.10",
        "style-loader": "^4.0.0",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.4.5",
        "vue-loader": "^17.3.1",
        "vue-template-compiler": "^2.7.15",
        "webpack": "^5.92.0",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "@vscode/codicons": "^0.0.35",
        "@vscode/webview-ui-toolkit": "^1.3.1",
        "digitaljs": "github:EDAcation/digitaljs#next",
        "edacation": "^0.3.7",
        "nextpnr-viewer": "^0.6.1",
        "os-browserify": "^0.3.0",
        "path-browserify": "^1.0.1",
        "tar-fs": "^3.0.6",
        "vue": "^3.4.27",
        "yosys2digitaljs": "^0.8.0"
    }
}
